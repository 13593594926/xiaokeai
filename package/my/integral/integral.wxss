.box .head {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #E85654;
  height: 200rpx;
}

.box .head view {
  font-size: 30rpx;
  color: #fff;
  margin-bottom: 20rpx;
}

.box .content {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 300rpx;
  color: #bbbbbb;
}
